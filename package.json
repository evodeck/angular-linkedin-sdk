        "angular",
        "angular 2",
        "angular 4",
        "linkedin",
        "linkedin sdk",
        "linkedin api",
        "api",
        "wrapper",
        "evodeck"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/evodeck/angular-linkedin-sdk/issues"
    },
    "main": "./index.js",
    "dependencies": {
        "rxjs-compat": "^6.0.0-rc.0"
    },
    "devDependencies": {
        "@angular/cli": "6.2.4",
        "@angular/common": "6.1.9",
        "@angular/compiler": "6.1.9",
        "@angular/compiler-cli": "6.1.9",
